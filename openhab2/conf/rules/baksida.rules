rule "Baksida evening on"
when
  Channel 'astro:sun:lampa:set#event' triggered START
then
  logInfo("Baksida", "Evening on")
  Baksida.sendCommand(ON)
end

rule "Baksida evening off"
when
  Time cron "0 37 22 * * ?"
then
  logInfo("Baksida", "Evening off")
  Baksida.sendCommand(OFF)
end

